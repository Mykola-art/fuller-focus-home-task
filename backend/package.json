{
  "name": "fuller-focus-home-task-backend",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "format": "prisma format && prettier --write \"src/**/*.ts\"",
    "client:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "prisma:db:push": "prisma db push --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@json2csv/plainjs": "^7.0.6",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.15",
    "prisma": "^5.22.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "prettier": "^3.7.4"
  }
}
